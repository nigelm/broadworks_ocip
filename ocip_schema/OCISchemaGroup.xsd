          <xs:element name="serviceProviderId" type="ServiceProviderId"/>
          <xs:element name="groupId" type="GroupId"/>
          <xs:element name="responsePagingControl" type="ResponsePagingControl" minOccurs="0"/>
          <xs:choice minOccurs="0">
            <xs:element name="sortByDeviceName" type="SortByDeviceName"/>
            <xs:element name="sortByDeviceType" type="SortByDeviceType"/>
            <xs:element name="sortByDeviceNetAddress" type="SortByDeviceNetAddress"/>
            <xs:element name="sortByDeviceMACAddress" type="SortByDeviceMACAddress"/>
