            <xs:sequence>
              <xs:element name="serviceProviderId" type="ServiceProviderId"/>
              <xs:element name="groupId" type="GroupId"/>
              <xs:element name="deviceName" type="AccessDeviceName"/>
            </xs:sequence>
            <xs:element name="deviceExternalId" type="ExternalId"/>
