  <xs:complexType name="CustomContactDirectoryEntry">
    <xs:annotation>
      <xs:documentation>
        Represents either an existing user's Id or an existing Virtual 
        On-Net user's DN. For a DN the groupId is used to make it unique 
        within an Enterprise, however the groupId is not used with Service 
        Providers.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:choice>
        <xs:element name="userId" type="UserId"/>
        <xs:sequence>
          <xs:element name="virtualOnNetPhoneNumber" type="DN"/>
          <xs:element name="groupId" type="GroupId" minOccurs="0"/>
        </xs:sequence>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="ReplacementCustomContactDirectoryEntryList">
    <xs:annotation>
      <xs:documentation>
        A list of userIds and/or Virtual On-Net user DNs that replaces a previously configured list.
        By convention, an element of this type may be set nill to clear the list.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="entry" type="CustomContactDirectoryEntry" minOccurs="1" maxOccurs="1000"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="SortOrderGroupGetListInServiceProviderPagedSortedList">
    <xs:annotation>
      <xs:documentation>
        Used to sort the GroupGetListInServiceProviderPagedSortedListRequest request.
      </xs:documentation>
    </xs:annotation>
    <xs:choice>
      <xs:element name="sortByGroupId" type="SortByGroupId"/>
      <xs:element name="sortByGroupName" type="SortByGroupName"/>
    </xs:choice>
  </xs:complexType>

  <xs:complexType name="SortOrderGroupPhoneDirectoryGetPagedSortedList">
    <xs:annotation>
      <xs:documentation>
        Used to sort the GroupPhoneDirectoryGetPagedSortedListRequest request.
      </xs:documentation>
    </xs:annotation>
    <xs:choice>
      <xs:element name="sortByUserLastName" type="SortByUserLastName"/>
      <xs:element name="sortByUserFirstName" type="SortByUserFirstName"/>
      <xs:element name="sortByMobilePhoneNumber" type="SortByMobilePhoneNumber"/>
      <xs:element name="sortByEmailAddress" type="SortByEmailAddress"/>
      <xs:element name="sortByDepartmentName" type="SortByDepartmentName"/>
      <xs:element name="sortByYahooId" type="SortByYahooId"/>
      <xs:element name="sortByUserId" type="SortByUserId"/>
      <xs:element name="sortByImpId" type="SortByImpId"/>
    </xs:choice>
  </xs:complexType>

  <xs:simpleType name="DepartmentCallingLineIdName">
    <xs:annotation>
      <xs:documentation>Department calling line ID name.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1"/>
      <xs:maxLength value="80"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="GroupDigitCollectionSettingLevel">
    <xs:annotation>
      <xs:documentation>
        Choices for the group digit collection feature to decide which type of settings to use.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="System"/>
      <xs:enumeration value="Service Provider"/>
      <xs:enumeration value="Group"/>
    </xs:restriction>
  </xs:simpleType>
</xs:schema>
