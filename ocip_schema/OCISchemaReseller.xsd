  <xs:complexType name="ResellerAddRequest">
    <xs:annotation>
      <xs:documentation>
        Add a reseller.
        The response is either SuccessResponse or ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="resellerId" type="ResellerId22"/>
          <xs:element name="resellerName" type="ResellerName22" minOccurs="0"/>
          <xs:element name="resellerExternalId" type="ExternalId" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerAdminAddRequest">
    <xs:annotation>
      <xs:documentation>
        Add a reseller administrator.
        The response is either a SuccessResponse or an ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="resellerId" type="ResellerId22"/>
          <xs:element name="userId" type="UserId"/>
          <xs:element name="firstName" type="FirstName" minOccurs="0"/>
          <xs:element name="lastName" type="LastName" minOccurs="0"/>
          <xs:element name="password" type="Password" minOccurs="0"/>
          <xs:element name="language" type="Language" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerAdminAlternateIdAddRequest">
    <xs:annotation>
      <xs:documentation>
        Request to add an alternate Id to a reseller admin.
        The response is either SuccessResponse or ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="userId" type="UserId"/>
          <xs:element name="alternateUserId" type="UserId"/>
          <xs:element name="description" type="AlternateUserIdDescription" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerAdminAlternateIdDeleteRequest">
    <xs:annotation>
      <xs:documentation>
        Request to delete an alternate admin user id of a reseller admin.
        The response is either SuccessResponse or ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="userId" type="UserId"/>
          <xs:element name="alternateUserId" type="UserId"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerAdminAlternateIdGetListRequest">
    <xs:annotation>
      <xs:documentation>
        Request to get the admin id and the list of alternate admin ids of an admin.
        The response is either ResellerAdminAlternateIdGetListResponse or ErrorResponse.
        The "userId" can be either the admin user Id or an alternate admin user Id.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="userId" type="UserId"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerAdminAlternateIdGetListResponse">
    <xs:annotation>
      <xs:documentation>
        Response to ResellerAdminAlternateIdGetListRequest.
        Contains a table of the main admin user id and the alternate admin user ids, the column headings are: "User Id", "Description", "Alternate".
        The possible values for "Alternate" are "true" and "false".
        The "Description" is only present for alternate admin user Ids.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIDataResponse">
        <xs:sequence>
          <xs:element name="adminUserIdTable" type="core:OCITable"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerAdminAlternateIdModifyRequest">
    <xs:annotation>
      <xs:documentation>
        Request to modify an alternate id of a reseller admin.
        The response is either SuccessResponse or ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="userId" type="UserId"/>
          <xs:element name="alternateUserId" type="UserId"/>
          <xs:element name="newAlternateUserId" type="UserId" minOccurs="0"/>
          <xs:element name="description" type="AlternateUserIdDescription" nillable="true" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerAdminDeleteRequest">
    <xs:annotation>
      <xs:documentation>
        Delete a reseller administrator.
        The response is either a SuccessResponse or an ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="userId" type="UserId"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerAdminGetListRequest">
    <xs:annotation>
      <xs:documentation>
        Get a list of reseller administrators.
        The response is either a ResellerAdminGetListResponse or an ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="resellerId" type="ResellerId22"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerAdminGetListResponse">
    <xs:annotation>
      <xs:documentation>
        Response to ResellerAdminGetListRequest.
        Contains a 4 column table with column headings "Administrator ID",
        "Last Name", "First Name", "Language".
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIDataResponse">
        <xs:sequence>
          <xs:element name="resellerAdminTable" type="core:OCITable"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerAdminGetRequest22">
    <xs:annotation>
      <xs:documentation>
        Get a reseller administrators profile.
        The response is either a ResellerAdminGetResponse22 or an ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="userId" type="UserId"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerAdminGetResponse22">
    <xs:annotation>
      <xs:documentation>
        Response to the ResellerAdminGetRequest22.
        The response contains the reseller administrators profile information.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIDataResponse">
        <xs:sequence>
          <xs:element name="resellerId" type="ResellerId22"/>
          <xs:element name="firstName" type="FirstName" minOccurs="0"/>
          <xs:element name="lastName" type="LastName" minOccurs="0"/>
          <xs:element name="language" type="Language"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerAdminModifyRequest">
    <xs:annotation>
      <xs:documentation>
        Modify the profile for a reseller administrator.
        Since old password is not specified here, any password rule related to old password does not apply.          
        The response is either a SuccessResponse or an ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="userId" type="UserId"/>
          <xs:element name="firstName" type="FirstName" nillable="true" minOccurs="0"/>
          <xs:element name="lastName" type="LastName" nillable="true" minOccurs="0"/>
          <xs:element name="password" type="Password" nillable="true" minOccurs="0"/>
          <xs:element name="language" type="Language" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerCallAdmissionControlPoliciesGetRequest">
    <xs:annotation>
      <xs:documentation>
        Get a reseller call admission control policies.
        The response is either a ResellerCallAdmissionControlPoliciesGetResponse or an ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="resellerId" type="ResellerId22"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerCallAdmissionControlPoliciesGetResponse">
    <xs:annotation>
      <xs:documentation>
        Response to the ResellerCallAdmissionControlPoliciesGetRequest.
        The response contains the reseller call admission control policies information.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIDataResponse">
        <xs:sequence>
          <xs:element name="enableCallAdmissionControl" type="xs:boolean"/>
          <xs:element name="maxConcurrentNetworkSessions" type="ConcurrentNetworkSessionsLimit"/>
          <xs:element name="maxConcurrentNetworkSessionsThreshold" type="ConcurrentNetworkSessionsThresholdLimit" minOccurs="0"/>
          <xs:element name="maxNetworkCallsPerSecond" type="NetworkCallsPerSecondLimit"/>
          <xs:element name="maxNetworkCallsPerSecondThreshold" type="NetworkCallsPerSecondThresholdLimit" minOccurs="0"/>
          <xs:element name="maxConcurrentExternalSIPRECSessions" type="ConcurrentExternalSIPRECSessionsLimit"/>
          <xs:element name="maxConcurrentExternalSIPRECSessionsThreshold" type="ConcurrentExternalSIPRECSessionsThresholdLimit" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerCallAdmissionControlPoliciesModifyRequest">
    <xs:annotation>
      <xs:documentation>
        Modify the reseller call admission control policies.
        The response is either a SuccessResponse or an ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="resellerId" type="ResellerId22"/>
          <xs:element name="enableCallAdmissionControl" type="xs:boolean" minOccurs="0"/>
          <xs:element name="maxConcurrentNetworkSessions" type="ConcurrentNetworkSessionsLimit" minOccurs="0"/>
          <xs:element name="maxConcurrentNetworkSessionsThreshold" type="ConcurrentNetworkSessionsThresholdLimit" minOccurs="0" nillable="true"/>
          <xs:element name="maxNetworkCallsPerSecond" type="NetworkCallsPerSecondLimit" minOccurs="0"/>
          <xs:element name="maxNetworkCallsPerSecondThreshold" type="NetworkCallsPerSecondThresholdLimit" minOccurs="0" nillable="true"/>
          <xs:element name="maxConcurrentExternalSIPRECSessions" type="ConcurrentExternalSIPRECSessionsLimit" minOccurs="0"/>
          <xs:element name="maxConcurrentExternalSIPRECSessionsThreshold" type="ConcurrentExternalSIPRECSessionsThresholdLimit" minOccurs="0" nillable="true"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerCallPoliciesGetRequest22">
    <xs:annotation>
      <xs:documentation>
        Request the reseller level data associated with Call Policies.
        The response is either a ResellerCallPoliciesGetResponse22
        or an ErrorResponse.
        
        Replaced by: ResellerCallPoliciesGetRequest22 in AS data mode.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="resellerId" type="ResellerId22"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerCallPoliciesGetResponse22">
    <xs:annotation>
      <xs:documentation>
        Response to ResellerCallPoliciesGetRequest22.
        
        Replaced by: ResellerCallPoliciesGetRequest22 in AS data mode.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIDataResponse">
        <xs:sequence>
          <xs:element name="forceRedirectingUserIdentityForRedirectedCalls" type="xs:boolean"/>
          <xs:element name="applyRedirectingUserIdentityToNetworkLocations" type="xs:boolean"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerCallPoliciesModifyRequest">
    <xs:annotation>
      <xs:documentation>
        Modify the reseller level data associated with Call Policies.
        The following elements are only used in AS data mode:
          forceRedirectingUserIdentityForRedirectedCalls
          applyRedirectingUserIdentityToNetworkLocations
          
        The response is either a SuccessResponse or an ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="resellerId" type="ResellerId22"/>
          <xs:element name="forceRedirectingUserIdentityForRedirectedCalls" type="xs:boolean" minOccurs="0"/>
          <xs:element name="applyRedirectingUserIdentityToNetworkLocations" type="xs:boolean" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerDeleteRequest">
    <xs:annotation>
      <xs:documentation>
        Delete a reseller.
        The response is either SuccessResponse or ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:choice>
            <xs:element name="resellerId" type="ResellerId22"/>
            <xs:element name="resellerExternalId" type="ExternalId"/>
          </xs:choice>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerDeviceActivationPolicyGetRequest">
    <xs:annotation>
      <xs:documentation>
        Request the Device Activation policy of a reseller.
        The response is either a ResellerDeviceActivationPolicyGetResponse or an ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="resellerId" type="ResellerId22"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerDeviceActivationPolicyGetResponse">
    <xs:annotation>
      <xs:documentation>
        Response to ResellerDeviceActivationPolicyGetRequest.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIDataResponse">
        <xs:sequence>
          <xs:element name="useResellerSettings" type="xs:boolean"/>
          <xs:element name="allowActivationCodeRequestByUser" type="xs:boolean"/>
          <xs:element name="sendActivationCodeInEmail" type="xs:boolean"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerDeviceActivationPolicyModifyRequest">
    <xs:annotation>
      <xs:documentation>
        Modify the Device Activation policy of a reseller.
        The response is either a SuccessResponse or an ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="resellerId" type="ResellerId22"/>
          <xs:element name="useResellerSettings" type="xs:boolean" minOccurs="0"/>
          <xs:element name="allowActivationCodeRequestByUser" type="xs:boolean" minOccurs="0"/>
          <xs:element name="sendActivationCodeInEmail" type="xs:boolean" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerEmergencyCallNotificationGetRequest">
    <xs:annotation>
      <xs:documentation>
        Request the reseller level data associated with Emergency Call 
        Notification.
        The response is either a 
        ResellerEmergencyCallNotificationGetResponse or an ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="resellerId" type="ResellerId22"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerEmergencyCallNotificationGetResponse">
    <xs:annotation>
      <xs:documentation>
        Response to ResellerEmergencyCallNotificationGetRequest.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIDataResponse">
        <xs:sequence>
          <xs:element name="defaultFromAddress" type="EmailAddress" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerEmergencyCallNotificationModifyRequest">
    <xs:annotation>
      <xs:documentation>
        Modify the reseller level data associated with Emergency Call 
        Notification.
        The response is either a SuccessResponse or an ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="resellerId" type="ResellerId22"/>
          <xs:element name="defaultFromAddress" type="EmailAddress" nillable="true" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerGetListRequest">
    <xs:annotation>
      <xs:documentation>
        Request a list of resellers in the system.
        It is possible to search by various criteria to restrict the number of rows returned.
        Multiple search criteria are logically ANDed together.
        The response is either a ResellerGetListResponse or an ErrorResponse.        
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="responseSizeLimit" type="ResponseSizeLimit" minOccurs="0"/>
          <xs:element name="searchCriteriaResellerId" type="SearchCriteriaResellerId" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="searchCriteriaResellerName" type="SearchCriteriaResellerName" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerGetListResponse">
    <xs:annotation>
      <xs:documentation>
        Response to the ResellerGetListRequest.
        Contains a 3 column table with column headings "Reseller Id", "Reseller Name" and "Reseller External Id". A row for each reseller.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIDataResponse">
        <xs:sequence>
          <xs:element name="resellerTable" type="core:OCITable"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerGetRequest22V2">
    <xs:annotation>
      <xs:documentation>
        Get the profile for a reseller.
        The response is either ResellerGetResponse22V2 or ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:choice>
            <xs:element name="resellerId" type="ResellerId22"/>
            <xs:element name="resellerExternalId" type="ExternalId"/>
          </xs:choice>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerGetResponse22V2">
    <xs:annotation>
      <xs:documentation>
        Response to the ResellerGetRequest22V2.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIDataResponse">
        <xs:sequence>
          <xs:element name="resellerName" type="ResellerName22" minOccurs="0"/>
          <xs:element name="resellerId" type="ResellerId22"/>
          <xs:element name="resellerExternalId" type="ExternalId" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerMailParametersGetRequest">
    <xs:annotation>
      <xs:documentation>
        Request the reseller Mail parameters.
        The response is either a ResellerMailParametersGetResponse or an
        ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="resellerId" type="ResellerId22"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerMailParametersGetResponse">
    <xs:annotation>
      <xs:documentation>
        Response to ResellerMailParametersGetRequest.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIDataResponse">
        <xs:sequence>
          <xs:element name="defaultFromAddress" type="EmailAddress" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerMailParametersModifyRequest">
    <xs:annotation>
      <xs:documentation>
        Request to modify the reseller Mail parameters.
        The response is either a SuccessResponse or an ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="resellerId" type="ResellerId22"/>
          <xs:element name="defaultFromAddress" type="EmailAddress" nillable="true" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerModifyRequest">
    <xs:annotation>
      <xs:documentation>
        Modify the reseller profile.
        The response is either SuccessResponse or ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:choice>
            <xs:element name="resellerId" type="ResellerId22"/>
            <xs:element name="resellerExternalId" type="ExternalId"/>
          </xs:choice>
          <xs:element name="resellerName" type="ResellerName22" nillable="true" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResellerNetworkClassOfServiceAssignListRequest">
    <xs:annotation>
      <xs:documentation>
        Assign a list of Network Classes of Service to a reseller.
        A default Network Class of Service is optional.
        The response is either a SuccessResponse or an ErrorResponse.
      </xs:documentation>
