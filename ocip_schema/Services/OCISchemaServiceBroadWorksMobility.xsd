          <xs:element name="isPrimary" type="xs:boolean" minOccurs="0"/>
          <xs:element name="enableAlerting" type="xs:boolean" minOccurs="0"/>
          <xs:element name="alertAgentCalls" type="xs:boolean" minOccurs="0"/>
          <xs:element name="alertClickToDialCalls" type="xs:boolean" minOccurs="0"/>
          <xs:element name="alertGroupPagingCalls" type="xs:boolean" minOccurs="0"/>
          <xs:element name="useMobilityCallingLineID" type="xs:boolean" minOccurs="0"/>
          <xs:element name="enableDiversionInhibitor" type="xs:boolean" minOccurs="0"/>
          <xs:element name="requireAnswerConfirmation" type="xs:boolean" minOccurs="0"/>
          <xs:element name="broadworksCallControl" type="xs:boolean" minOccurs="0"/>
          <xs:element name="useSettingLevel" type="BroadWorksMobilityUserSettingLevel" minOccurs="0"/>
          <xs:element name="denyCallOriginations" type="xs:boolean" minOccurs="0"/>
          <xs:element name="denyCallTerminations" type="xs:boolean" minOccurs="0"/>
          <xs:element name="devicesToRing" type="BroadWorksMobilityPhoneToRing" minOccurs="0"/>
          <xs:element name="includeSharedCallAppearance" type="xs:boolean" minOccurs="0"/>
          <xs:element name="includeBroadworksAnywhere" type="xs:boolean" minOccurs="0"/>
          <xs:element name="includeExecutiveAssistant" type="xs:boolean" minOccurs="0"/>
          <xs:element name="mobileNumbersAlerted" type="BroadWorksMobilityAlertingMobileNumberReplacementList" minOccurs="0" nillable="true"/>
          <xs:element name="enableCallAnchoring" type="xs:boolean" minOccurs="0"/>
          <xs:element name="timeSchedule" type="ScheduleGlobalKey" minOccurs="0" nillable="true"/>
          <xs:element name="holidaySchedule" type="ScheduleGlobalKey" minOccurs="0" nillable="true"/>
          <xs:choice minOccurs="0">
            <xs:element name="accessDeviceEndpoint" type="AccessDeviceEndpointAndHotlineModify" nillable="true"/>
            <xs:element name="outboundAlternateNumber" type="OutgoingDNorSIPURI" nillable="true"/>
          </xs:choice>
          <xs:element name="enableDirectRouting" type="xs:boolean" minOccurs="0"/>
          <xs:element name="markCDRAsEnterpriseGroupCalls" type="xs:boolean" minOccurs="0"/>
          <xs:element name="useMobilityConnectedIdentity" type="xs:boolean" minOccurs="0"/>
          <xs:element name="networkTranslationIndex" type="NetworkTranslationIndex" minOccurs="0" nillable="true"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

