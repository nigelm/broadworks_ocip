    <xs:restriction base="xs:token">
      <xs:enumeration value="None"/>
      <xs:enumeration value="Busy"/>
      <xs:enumeration value="Transfer"/>
      <xs:enumeration value="Night Service"/>
      <xs:enumeration value="Ringing"/>
      <xs:enumeration value="Announcement"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="CallCenterThresholdCrossingNotificationEmailGuardTimerSeconds">
    <xs:annotation>
      <xs:documentation>
         The time determines how long the system waits and prevents notification emails being 
         generated as a statistic swings back and forth across a threshold.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:int">
      <xs:minInclusive value="3"/>
      <xs:maxInclusive value="60"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="CallCenterUniformCallDistributionPolicyScope">
    <xs:annotation>
      <xs:documentation>
        Call Center Uniform Call Distribution Policy Scope.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="Agent"/>
      <xs:enumeration value="Call Center"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="CallCenterWaitingTimeNotificationThresholdSeconds">
    <xs:annotation>
      <xs:documentation>
          Waiting time for a given call before notification to agents devices.
	    </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:int">
      <xs:minInclusive value="1"/>
      <xs:maxInclusive value="7200"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="CallDispositionCode">
    <xs:annotation>
      <xs:documentation>
        Call Center Call Disposition Code Value.
	  </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1"/>
      <xs:maxLength value="10"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="CallDispositionCodeDescription">
    <xs:annotation>
      <xs:documentation>
        Call Center Call Disposition Code Description.
	  </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1"/>
      <xs:maxLength value="40"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="CallDispositionCodeLevel">
    <xs:annotation>
      <xs:documentation>
        Identifies at which level in the system an call center call disposition code is created.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="Enterprise"/>
      <xs:enumeration value="Group"/>
      <xs:enumeration value="Queue"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="CallHandlingMinimumSamplingSize">
    <xs:annotation>
      <xs:documentation>
         The minimum number of entries for the call handling time moving average to be valid.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:int">
      <xs:minInclusive value="1"/>
      <xs:maxInclusive value="100"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="CallHandlingSamplingPeriodMinutes">
    <xs:annotation>
      <xs:documentation>
         The moving average period used to calculate the call handling time.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:int">
      <xs:minInclusive value="5"/>
      <xs:maxInclusive value="30"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="DNISPriority">
    <xs:annotation>
      <xs:documentation>
        Choices for DNIS priority.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="0 - Highest"/>
      <xs:enumeration value="1 - High"/>
      <xs:enumeration value="2 - Medium"/>
      <xs:enumeration value="3 - Low"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="DNISPromoteCallPrioritySeconds">
    <xs:annotation>
      <xs:documentation>
        Timer to upgrade the priority for a DNIS number 
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:int">
      <xs:minInclusive value="1"/>
      <xs:maxInclusive value="1800"/>
    </xs:restriction>
  </xs:simpleType>
</xs:schema>
