            <xs:element name="resellerId" type="ResellerId22" minOccurs="0"/>
          </xs:choice>
          <xs:element name="excludeLeafDeviceTypes" type="xs:boolean" fixed="true" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

