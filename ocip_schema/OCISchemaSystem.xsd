              <xs:element name="groupId" type="GroupId"/>
            </xs:sequence>
          </xs:choice>
          <xs:element name="fromDeviceType" type="AccessDeviceType"/>
          <xs:element name="toDeviceType" type="AccessDeviceType"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

