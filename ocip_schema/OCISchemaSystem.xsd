          <xs:element name="allowMusicOnHold" type="xs:boolean"/>
          <xs:element name="onlyConference" type="xs:boolean"/>
          <xs:element name="onlyVideoCapable" type="xs:boolean"/>
          <xs:element name="onlyOptionalIpAddress" type="xs:boolean"/>
