            <xs:sequence>
              <xs:element name="svcProviderId" type="ServiceProviderId"/>
              <xs:element name="groupId" type="GroupId"/>
            </xs:sequence>
          </xs:choice>
          <xs:element name="deviceName" type="AccessDeviceName"/>
          <xs:element name="toDeviceType" type="AccessDeviceType"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="SystemDeviceManagementSwitchDeviceTypeRequest">
    <xs:annotation>
