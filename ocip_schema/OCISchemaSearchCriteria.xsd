  <xs:complexType name="SearchCriteriaComposedOrUserName">
    <xs:annotation>
      <xs:documentation>
        Criteria for searching for a user's full name.
        This search criterion will be compared against multiple combinations of first name and last name:

        First Name + ‘ ‘ + Last Name
        Last Name + ‘ ‘ + First Name
        Hiragana Last Name + ' ' + Hiragana First Name

        Note: For this search criterion, the searchMode is always ‘Contains’ and the search criteria are always OR’ed.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="SearchCriteriaComposedOr">
        <xs:sequence>
          <xs:element name="value" minOccurs="1" maxOccurs="unbounded">
            <xs:simpleType>
              <xs:annotation/>
              <xs:restriction base="xs:token">
                <xs:minLength value="1"/>
                <xs:maxLength value="255"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
          <xs:element name="isCaseInsensitive" type="xs:boolean" default="true"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

